out_to_console yes
out_to_x no
background no
update_interval 2
total_run_times 0
use_spacer none
# indicate up when having a link
if_up_strictness link

TEXT

#wlan
${if_match ${battery_percent BAT1} < 10} ${blink ${battery_short BAT1}}\
${else} ${battery_short BAT1}\
${endif}\
 - ${battery_time BAT1} :: ${cpu cpu1}% / ${cpu cpu2}% / ${cpu cpu3}% / ${cpu cpu4}% :: $memperc% ($mem) ::\
${if_up wlan0} W(${wireless_link_qual_perc wlan0}%) dwn: ${downspeed wlan0} up: ${upspeed wlan0}\
${else} C dwn: ${downspeed eth0} up: ${upspeed eth0}\
${endif}\
 :: ${time %a %b %d %H:%M}
